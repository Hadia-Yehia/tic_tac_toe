      </ImageView>
      <Button fx:id="gotolevels" layoutX="439.0" layoutY="304.0" mnemonicParsing="false" onAction="#gotolevels" prefHeight="36.0" prefWidth="87.0" style="-fx-background-radius: 15px; -fx-background-color: #EFEDE3;" text="Next">
