<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="version.pkg1.welcom.page.LevelScreenController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
         <image>
            <Image url="@WhatsApp%20Image%202022-12-27%20at%206.17.11%20PM.jpeg" />
         </image>
      </ImageView>
      <Label layoutX="208.0" layoutY="45.0" text="Select Level" textFill="#54cba7">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Label>
      <Button fx:id="Beginner" layoutX="215.0" layoutY="119.0" mnemonicParsing="false" onAction="#GameScreen" prefHeight="46.0" prefWidth="175.0" style="-fx-background-radius: 20px; -fx-background-color: beige;" text="Beginner">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></Button>
      <Button fx:id="Proficient" layoutX="214.0" layoutY="206.0" mnemonicParsing="false" onAction="#GameScreenExpert" prefHeight="46.0" prefWidth="175.0" style="-fx-background-radius: 20px; -fx-background-color: beige;" text="Proficient">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></Button>
      <Button fx:id="GameScreenExpert" layoutX="212.0" layoutY="282.0" mnemonicParsing="false" onAction="#GameScreenExpert" prefHeight="46.0" prefWidth="175.0" style="-fx-background-radius: 20px; -fx-background-color: beige;" text="Expert">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></Button>
      <Button fx:id="homescreen" layoutX="50.0" layoutY="54.0" mnemonicParsing="false" onAction="#homescreen" style="-fx-background-color: #EFEDE3;">
         <graphic>
            <ImageView fitHeight="39.0" fitWidth="54.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@home_ic.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
